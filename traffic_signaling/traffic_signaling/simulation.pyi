from .city_plan import CityPlan

class Schedule:
    def set_schedule(self, times:list[int], order:list[int]) -> None: ...
    @property
    def length(self) -> int: ...
    @property
    def duration(self) -> int: ...

class Simulation:
    def __init__(self, city_plan:CityPlan) -> None: ...
    def read_schedules(self, filename:str) -> None: ...
    def write_schedules(self, filename:str) -> None: ...
    def default_schedules(self) -> None: ...
    def update_schedules(
        self, 
        schedules:dict[int, tuple[list[int], list[int]]]
    ) -> None: ...
    def score(self) -> int: ...
    def summary(self) -> None: ...
    @property
    def schedules(self) -> dict[int, Schedule]: ...
    @property
    def city_plan(self) -> CityPlan: ...
